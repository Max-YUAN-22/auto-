{
  "version": 2,
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "REACT_APP_BACKEND_URL": "https://multi-agent-dsl-backend.railway.app",
    "REACT_APP_VERSION": "v2.0.0-enterprise",
    "REACT_APP_ENVIRONMENT": "production"
  }
}
